{
  "name": "nerthusaddon",
  "version": "11.2.0",
  "description": "Addon for the Nerthus server in game [Margonem](https://www.margonem.pl/)",
  "main": "/src/main.js",
  "directories": {
    "test": "test"
  },
  "browserslist": [
    "defaults",
    "firefox 115"
  ],
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@tsconfig/recommended": "^1.0.13",
    "@tsconfig/strictest": "^2.0.8",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "jquery": "3.6.0",
    "js-yaml": "^4.1.1",
    "omggif": "^1.0.10",
    "ts-loader": "^9.5.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitest/coverage-v8": "^3.2.4",
    "coveralls": "^3.1.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "expect.js": "^0.3.1",
    "exports-loader": "^5.0.0",
    "expose-loader": "^5.0.1",
    "globals": "^17.0.0",
    "http-server": "^14.1.1",
    "imports-loader": "^5.0.0",
    "jsdom": "^27.4.0",
    "mocha": "^11.7.5",
    "mochapack": "^2.1.5",
    "nyc": "^17.1.0",
    "prettier": "3.7.4",
    "sinon": "^21.0.1",
    "vitest": "^3.2.4",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "test-code-quality": "eslint src/*",
    "build": "webpack",
    "build-watch": "webpack --watch",
    "update-version": "git rev-parse --short HEAD > version",
    "dev-serve": "http-server ./dist -c-1 --cors",
    "dev-setup": "node scripts/setup.mjs",
    "dev-start": "npm run dev-serve & npm run build-watch"
  },
  "overrides": {
    "tough-cookie": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerthus-margonem/nerthusaddon.git"
  },
  "bugs": {
    "url": "https://github.com/nerthus-margonem/nerthusaddon/issues"
  },
  "homepage": "https://github.com/nerthus-margonem/nerthusaddon#readme",
  "license": "MIT"
}
